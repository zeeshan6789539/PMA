{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express backend with PostgreSQL and Drizzle ORM",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "esbuild src/server.ts --bundle --platform=node --format=esm --outdir=dist --packages=external && tsc",
    "db:push": "drizzle-kit push --config=src/config/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=src/config/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=src/config/drizzle.config.ts",
    "db:seed": "tsx src/database/seeders/index.ts"
  },
  "keywords": [
    "express",
    "postgresql",
    "drizzle",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
